<body id="outstanding">

<h1>List of outstanding shoe orders</h1>

<table>
<tr id="headers">
    <th>Name</th><th>Address</th><th>Quantity</th><th>Size</th>
</tr>

<% @shoes.each do |shoe| %>
<tr>
    <td><%= shoe.name %></td>
    <td><%= shoe.address %></td>
    <td><%= shoe.quantity %></td>
    <td><%= shoe.size %></td>
</tr>

<% end %>

</table>